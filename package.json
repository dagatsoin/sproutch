{
  "name": "@sproutch/ui",
  "version": "0.0.3",
  "description": "A cross platform UI kit for web and react native Android/iOS.",
  "author": "Daniel Neveux",
  "homepage": "https://dagatsoin.github.io/sproutch/",
  "repository": "https://github.com/dagatsoin/sproutch",
  "bugs": {
    "url": "https://github.com/dagatsoin/sproutch/issues"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "index.native.js",
    "index.native.d.ts",
    "lib",
    "LICENSE"
  ],
  "license": "MIT",
  "keywords": [
    "reactnative",
    "reactxp",
    "uikit",
    "material-ui",
    "material-design"
  ],
  "watch": {
    "publish-local": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx,ts,tsx,json,md,css",
      "ignore": "src/node_modules",
      "legacyWatch": true,
      "delay": 2500,
      "runOnChangeOnly": false
    }
  },
  "scripts": {
    "build": "npm run clean && npm run tslint-check && npm run lint && npm run tsc",
    "publish-local": "yalc publish",
    "clean": "rimraf lib",
    "lint": "tslint --config tslint.prod.json --project .",
    "prepare": "npm run build",
    "pretty-quick": "pretty-quick --staged --pattern \"src/**/*.*(js|jsx|ts|tsx|json|md|css)\"",
    "tsc": "tsc",
    "tslint-check": "tslint-config-prettier-check ./tslint.prod.json",
    "watch": "npm-watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pretty-quick"
    }
  },
  "dependencies": {
    "lodash.merge": "^4.6.1"
  },
  "peerDependencies": {
    "expo": "^32.0.0",
    "reactxp": "^1.6.0-rc.3"
  },
  "devDependencies": {
    "@types/expo": "^32.0.0",
    "@types/node": "^11.9.5",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-native": "^0.57.23",
    "awesome-typescript-loader": "^5.2.1",
    "babel-preset-expo": "^5.0.0",
    "expo": "^32.0.0",
    "husky": "^1.3.1",
    "npm-watch": "^0.6.0",
    "prettier": "^1.16.3",
    "pretty-quick": "^1.10.0",
    "react": "16.5.1",
    "react-art": "^16.7.0",
    "reactxp": "^1.6.0-rc.1",
    "rimraf": "^2.6.2",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2"
  }
}
