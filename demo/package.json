{
  "main": "./node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "rnstl && expo start",
    "android": "rnstl && expo start --android",
    "ios": "rnstl && expo start --ios",
    "postinstall": "rndebugger-open --expo",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./stories"
      ],
      "pattern": "**/story.native.tsx",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "dependencies": {
    "@sproutch/ui": "file:.yalc/@sproutch/ui",
    "@storybook/addon-actions": "^4.1.11",
    "@storybook/addon-knobs": "^4.1.11",
    "@storybook/addon-links": "^4.1.11",
    "@storybook/addon-notes": "^4.1.11",
    "@storybook/addon-ondevice-knobs": "^4.1.11",
    "@storybook/addon-ondevice-notes": "^4.1.11",
    "@storybook/addons": "^4.1.11",
    "@storybook/react": "^4.1.11",
    "@storybook/react-native": "^4.1.11",
    "@storybook/storybook-deployer": "^2.8.1",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "core-js": "^2.6.3",
    "deepmerge": "^3.1.0",
    "expo": "^32.0.0",
    "lodash.merge": "^4.6.1",
    "react": "16.5.1",
    "react-art": "^16.8.1",
    "react-dom": "16.5.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-32.0.0.tar.gz",
    "react-native-debugger-open": "^0.3.17",
    "react-native-storybook-loader": "^1.8.0",
    "react-native-vector-icons": "^6.2.0",
    "react-native-web": "^0.10.0",
    "reactxp": "^1.5.0"
  }
}
