{
  "name": "@sproutch/ui",
  "version": "0.0.5-alpha.0",
  "description": "A cross platform UI kit for web and react native Android/iOS.",
  "author": "Daniel Neveux",
  "homepage": "https://dagatsoin.github.io/sproutch/",
  "repository": "https://github.com/dagatsoin/sproutch",
  "bugs": {
    "url": "https://github.com/dagatsoin/sproutch/issues"
  },
  "engines": {
    "npm": ">=8.11.0 <9.0.0",
    "node": ">=17.9.1 <18.0.0"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "index.native.js",
    "index.native.d.ts",
    "lib",
    "LICENSE"
  ],
  "license": "MIT",
  "keywords": [
    "reactnative",
    "reactxp",
    "uikit",
    "material-ui",
    "material-design"
  ],
  "watch": {
    "publish-local": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx,ts,tsx,json,md,css",
      "ignore": "src/node_modules",
      "legacyWatch": true,
      "delay": 2500,
      "runOnChangeOnly": false
    }
  },
  "scripts": {
    "build": "npm run clean && npm run lint && npm run compile",
    "compile": "tsc --project prod.tsconfig.json",
    "dev": "tsc --watch",
    "clean": "rimraf lib",
    "lint": "eslint --config .eslintrc.json 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint --fix --config .eslintrc.json 'src/**/*.{ts,tsx}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pretty-quick"
    }
  },
  "dependencies": {
    "@react-spring/native": "^9.7.3",
    "@react-spring/web": "^9.7.3",
    "lodash.merge": "^4.6.2"
  },
  "peerDependencies": {
    "expo": "^49.0.13"
  },
  "devDependencies": {
    "@types/lodash.merge": "^4.6.7",
    "@types/react-dom": "^18.2.11",
    "@types/react-native": "^0.72.3",
    "expo": "^49.0.13",
    "react-dom": "^18.2.0",
    "react-native-web": "^0.19.9",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2"
  }
}
